{
  "name": "connect-me",
  "version": "1.0.0",
  "description": "Allowers friends from all over the world chat and also send money to one another",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=html mocha --compilers js:babel-register ./server/tests/* --no-deprecation  --timeout 1000000 --exit  && nyc report",
    "start": "node ./dist/server/app.js",
    "start:pm2": "./node_modules/pm2/bin/pm2 start pm2-apps.json && DEBUG=log nodemon --watch server --exec babel-node ./server/app.js",
    "start:dev": "nodemon --exec babel-node ./server/app.js",
    "pm2:logs": "node ./node_modules/pm2/bin/pm2 logs",
    "build": "babel server -d dist/server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otutudinma/connect-me.git"
  },
  "keywords": [
    "Chat",
    "money",
    "friends"
  ],
  "author": "Chidinma Otutu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/otutudinma/connect-me/issues"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.2",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cloudinary": "^1.13.2",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "elasticsearch": "^15.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "firebase-admin": "^6.5.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "imagemin": "^6.0.0",
    "jsonwebtoken": "^8.5.0",
    "md5": "^2.2.1",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "mongoose": "^5.3.12",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "node-forge": "^0.7.6",
    "nodemailer": "^4.7.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "pm2": "^3.2.2",
    "qr-image": "^3.2.0",
    "qrcode": "^1.3.2",
    "ravepay": "^1.2.0",
    "redis": "^2.8.0",
    "shortid": "^2.2.14",
    "socket.io": "^2.2.0",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.5.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  },
  "homepage": "https://github.com/otutudinma/connect-me#readme"
}
